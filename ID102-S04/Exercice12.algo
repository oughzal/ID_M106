Algorithme Exe12
Var
    T1,T2,T3 : Tableau[100] de Entier
    i,N : Entier
Début
	    //Remplir les tableaux T1 et T2
	    T1 =[4,8,7,9,1,5,4,6]
	    T2 = [7,6,5,2,1,3,7,4]
	    N = 8
	    //T3 = [11,14,12,11,2,8,11,10]
	    pour i de 0 à N-1 Faire
	    	        T3[i] = T1[i] + T2[i]
	    finpour
	    //Afficher le tableau
	    Ecrire("T3 : [")
	    pour i de 0 à N-1 Faire
	    	        Ecrire(T3[i],",")
	    finpour
	    Ecrireln("]")
Fin
